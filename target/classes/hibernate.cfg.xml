<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory >
  <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
  <property name="hibernate.connection.password">12345</property>
  <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/ventacoches</property>
  <property name="hibernate.connection.username">ventacoches</property>
  <property name="hibernate.default_schema">ventacoches</property>
  <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
  <mapping resource="models/AgenciaVentaCoche.hbm.xml"/>
  <mapping resource="models/Anuncio.hbm.xml"/>
  <mapping resource="models/Ciudades.hbm.xml"/>
  <mapping resource="models/Pais.hbm.xml"/>
  <mapping resource="models/Vehiculo.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
